.page {
  min-height: 100vh;
  width: 100%;
  background-color: #f8f8f8;
}
.page .items {
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.page .items .item {
  height: 200rpx;
  width: 100%;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
